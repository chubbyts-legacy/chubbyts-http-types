{
  "name": "@chubbyts/chubbyts-http-types",
  "type": "module",
  "version": "2.1.0",
  "description": "Http Request/Response types with middleware and handler, PSR-15 inspired.",
  "keywords": [
    "chubbyts",
    "request",
    "server-request",
    "response",
    "middleware",
    "handler",
    "psr-15"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-http-types",
  "scripts": {
    "build": "node ./build.mjs",
    "cs-fix": "prettier --write src tests",
    "cs": "prettier --check src tests",
    "infection": "stryker run",
    "lint-fix": "eslint src tests eslint.config.mjs vitest.config.ts --fix",
    "lint": "eslint src tests",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./*.d.ts",
      "require": "./*.cjs",
      "import": "./*.mjs",
      "default": "./*.mjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-eslint": "^3.0.4",
    "@chubbyts/chubbyts-packaging": "^4.0.0",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/vitest-runner": "^8.7.1",
    "@types/node": "^22.13.1",
    "@types/qs": "^6.9.18",
    "@vitest/coverage-v8": "^3.0.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
