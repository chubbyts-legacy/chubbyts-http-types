{
  "name": "@chubbyts/chubbyts-http-types",
  "type": "module",
  "version": "1.2.0",
  "description": "Http Request/Response types with middleware and handler, PSR-15 inspired.",
  "keywords": [
    "chubbyts",
    "request",
    "server-request",
    "response",
    "middleware",
    "handler",
    "psr-15"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-http-types",
  "scripts": {
    "build": "rm -rf ./dist && tsc -b ./tsconfig.types.json ./tsconfig.esm.json ./tsconfig.cjs.json && node ./rename-commonjs.cjs",
    "cs-fix": "prettier --write src",
    "cs": "prettier --check src",
    "lint-fix": "eslint src --fix",
    "lint": "eslint src",
    "prepare": "rm -Rf dist && npm run build && npm run cs"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-eslint": "^2.0.2",
    "@types/node": "^20.4.4",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
